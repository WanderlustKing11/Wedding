<header>
  <nav>
    <div class='mobile-menu-toggle'>
      <button
        class='hamburger'
        aria-label='Toggle mobile menu'
        aria-expanded='false'
        aria-controls='mobile-menu'
      >
        <div class='bar'></div>
      </button>
    </div>
    <ul class='menu' role='menubar'>
      <% const pages = ['Home', 'About Us', 'RSVP', 'Photos', 'Details']; %> 
      <% pages.forEach(page => { %>
        <% if (page === 'Home') { %>
          <a href='/' role='menuitem'><%= page %></a>
        <% } else { %>
          <a href="/<%= page.toLowerCase().replace(/\s+/g, '-') %>" role='menuitem'><%= page %></a>
        <% } %>
      <% }); %>
    </ul>
  </nav>
  <nav class='mobile-menu' role='menu' aria-hidden='true'>
    <% pages.forEach(page => { %>
      <% if (page === 'Home') { %>
        <a href='/' role='menuitem'><%= page %></a>
      <% } else { %>
        <a href="/<%= page.toLowerCase().replace(/\s+/g, '-') %>" role='menuitem'><%= page %></a>
      <% } %>
    <% }); %>
  </nav>
  <h1 class='title'>Heather & Doug</h1>
</header>
